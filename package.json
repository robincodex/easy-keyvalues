{
    "name": "easy-keyvalues",
    "version": "1.1.2",
    "description": "Parse Valve KeyValues Format and easy to use in nodejs or browser.",
    "main": "dist/node.js",
    "engines": {
        "node": ">=16.0.0"
    },
    "engineStrict": true,
    "scripts": {
        "watch": "rollup -c -w --bundleConfigAsCjs",
        "build": "rollup -c --bundleConfigAsCjs",
        "test": "jest --verbose",
        "test:badges": "jest --verbose && jest-coverage-badges",
        "test:kv": "jest --coverage=false -- __tests__/KeyValues.test.ts",
        "test:kv3": "jest --coverage=false -- __tests__/KeyValues3.test.ts",
        "lint:prettier": "prettier --check .",
        "write-prettier": "prettier --write ."
    },
    "author": "RobinCode",
    "homepage": "https://github.com/RobinCodeX/easy-keyvalues",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/RobinCodeX/easy-keyvalues.git"
    },
    "keywords": [
        "KeyValues",
        "KeyValues3",
        "Valve KeyValues",
        "Source2 KeyValues3"
    ],
    "license": "MIT",
    "dependencies": {
        "chardet": "^1.5.1",
        "iconv-lite": "^0.6.3"
    },
    "devDependencies": {
        "@jest/globals": "^29.5.0",
        "@rollup/plugin-commonjs": "25.0.0",
        "@rollup/plugin-node-resolve": "15.0.2",
        "@rollup/plugin-typescript": "11.1.1",
        "@types/jest": "^29.5.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.2.3",
        "jest": "^29.5.0",
        "jest-coverage-badges": "^1.1.2",
        "prettier": "^2.8.8",
        "rollup": "3.23.0",
        "rollup-plugin-dts": "^5.3.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.2",
        "typescript": "5.0.4"
    }
}
